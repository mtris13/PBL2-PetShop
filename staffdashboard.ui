   <string notr="true">/* ----- STYLE CHO STAFF DASHBOARD (GIAO DIỆN VÀNG/XANH) ----- */

/* Cột Trái (NavBar) - MÀU VÀNG (Giống Client) */
QWidget#navBar {
    background-color: #FDD85D; 
    border-right: 1px solid #E0E0E0;
}

/* Các nút trong NavBar */
QWidget#navBar QPushButton {
    background-color: transparent; 
    border: none; 
    color: #333333; /* Chữ xám đậm */
    font-size: 16px;
    font-weight: bold;
    padding: 15px; 
    text-align: left; 
}

QWidget#navBar QPushButton:hover {
    background-color: #FBCB2E; /* Vàng đậm hơn */
}


/* Header Bar - MÀU XANH DƯƠNG (Giống Client) */
QWidget#headerBar {
    background-color: #5D9CFD; 
    padding: 10px;
}

#headerBar QLabel {
    color: #FFFFFF; /* Chữ trắng */
}

#welcomeTitleLabel, #userNameLabel {
    font-size: 18px;
    font-weight: bold;
}

#userRoleLabel {
    font-size: 14px;
    font-style: italic;
}

/* Content Area */
QWidget#contentArea {
    background-color: #FFFFFF;
}
/* ----- TRANG TẠO KHÁCH HÀNG (ĐẸP) ----- */

/* Nền trang màu vàng */
#createClientPage {
    background-color: #FDD85D; 
}

/* Hộp Form màu trắng ở giữa */
#clientFormContainer {
    background-color: #FFFFFF;
    border-radius: 15px; /* Bo tròn góc */
    padding: 30px; /* Khoảng cách nội dung với viền */
    /* Tạo hiệu ứng bóng đổ nhẹ (tùy chọn) */
    border: 1px solid #E0E0E0;
}

/* Tiêu đề Form */
#formTitleLabel {
    font-size: 24px;
    font-weight: bold;
    color: #333333;
    margin-bottom: 20px;
}

/* Các ô nhập liệu */
#clientFormContainer QLineEdit {
    background-color: #F8F8F8;
    border: 1px solid #CCC;
    border-radius: 10px;
    padding: 12px;
    font-size: 14px;
    margin-bottom: 10px; /* Cách nhau ra */
}
#clientFormContainer QLineEdit:focus {
    border: 2px solid #5D9CFD; /* Viền xanh khi gõ */
    background-color: #FFFFFF;
}

/* GroupBox Giới tính */
#clientFormContainer QGroupBox {
    border: 1px solid #CCC;
    border-radius: 10px;
    margin-top: 10px;
    padding: 15px;
    font-weight: bold;
    color: #555;
}
#clientFormContainer QGroupBox::title {
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 5px;
}

/* Nút &quot;TẠO TÀI KHOẢN&quot; */
#saveClientButton {
    background-color: #5D9CFD; /* Màu xanh dương */
    color: #FFFFFF;
    font-weight: bold;
    font-size: 16px;
    border-radius: 10px;
    padding: 12px;
    margin-top: 15px;
}
#saveClientButton:hover {
    background-color: #4A90E2; /* Xanh đậm hơn */
}
/* ... (Các code cũ giữ nguyên) ... */

/* ----- SỬA MÀU CHỮ CHO FORM TẠO KHÁCH HÀNG ----- */

/* Đặt màu chữ đen cho tất cả các thành phần trong hộp nhập liệu */
#clientFormContainer QLabel, 
#clientFormContainer QLineEdit, 
#clientFormContainer QGroupBox, 
#clientFormContainer QRadioButton {
    color: #333333; /* Màu xám đen dễ đọc */
}

/* Đảm bảo tiêu đề form cũng màu đậm */
#formTitleLabel {
    color: #006064; /* Xanh đậm (như cũ) */
    font-weight: bold;
}

/* Input khi gõ vào cũng phải màu đen */
#clientFormContainer QLineEdit {
    color: #000000; 
    background-color: #F8F8F8; /* Nền xám nhạt */
    border: 1px solid #CCC;
}
/* Cột Trái (Kho hàng) */
#posLeftPanel {
    background-color: #FFFFFF;
    padding: 10px;
}

/* Cột Phải (Thanh toán) */
#posRightPanel {
    background-color: #E0F2F1; /* Màu xanh ngọc nhạt */
    border-left: 2px solid #009688;
    padding: 15px;
}

/* Tiêu đề */
#posPetsTitle, #posClientNameLabel {
    font-size: 18px;
    font-weight: bold;
    color: #006064;
    margin-bottom: 10px;
}

/* Ô nhập SĐT */
#posPhoneInput, #spaPhoneInput {
    border: 2px solid #009688;
    border-radius: 5px;
    padding: 8px;
    font-size: 14px;
}

/* Nút Thanh toán */
#posPayButton {
    background-color: #FF5722; /* Màu CAM ĐẬM */
    color: white;
    font-size: 18px;
    font-weight: bold;
    border-radius: 10px;
    padding: 15px;
    margin-top: 20px;
}
#posPayButton:hover {
    background-color: #E64A19;
}
#posRightPanel QLabel {
    color: #004D40; /* Xanh rất đậm (gần như đen) */
    font-size: 14px;
}

/* Các Tiêu đề (In đậm, to hơn) */
#posInfoTitleLabel, #posCartTitleLabel {
    font-size: 18px;
    font-weight: bold;
    color: #006064; /* Xanh đậm */
    margin-bottom: 5px;
    margin-top: 10px;
}

/* Label &quot;Customer:&quot; */
#posCustomerLabel {
    font-weight: bold;
}

/* Tên Khách hàng (Kết quả tìm kiếm) */
#posClientNameLabel {
    font-weight: bold;
    color: #D81B60; /* Màu hồng đậm để nổi bật */
    font-size: 15px;
    margin-bottom: 10px;
}

/* Tổng tiền (To, Đỏ) */
#posTotalLabel {
    font-size: 20px;
    font-weight: bold;
    color: #D32F2F; /* Đỏ đậm */
    margin-top: 10px;
    qproperty-alignment: 'AlignRight'; /* Căn phải */
}

/* List Widget (Giỏ hàng tạm) */
#posCartList {
    background-color: #FFFFFF;
    color: #000000; /* Chữ đen */
    border: 1px solid #009688;
    border-radius: 5px;
}
/* ----- POS SPA (TƯƠNG TỰ PETS) ----- */

/* Cột Trái */
#spaLeftPanel {
    background-color: #FFFFFF;
    padding: 10px;
}

/* Cột Phải */
#spaRightPanel {
    background-color: #E3F2FD; /* Xanh dương rất nhạt (khác chút với Pets) */
    border-left: 2px solid #2196F3;
    padding: 15px;
}

/* Tiêu đề */
#spaClientNameLabel, #spaTotalLabel, #posInfoTitleLabel_2, #label_2, #posSpaTitle {
    font-weight: bold;
    color: #D81B60;
    font-size: 15px;
    margin-bottom: 10px;
}

/* Nút Đặt lịch */
#spaBookButton {
    background-color: #2196F3; /* Màu XANH DƯƠNG */
    color: white;
    font-size: 16px;
    font-weight: bold;
    border-radius: 8px;
    padding: 15px;
    margin-top: 20px;
}
#spaBookButton:hover {
    background-color: #1976D2;
}
/* ----- DANH SÁCH GIỎ HÀNG SPA ----- */
#spaCartList {
    background-color: #FFFFFF; /* Nền trắng */
    color: #000000; /* Chữ đen (Quan trọng!) */
    border: 1px solid #2196F3;
    border-radius: 5px;
    font-size: 14px;
    font-weight: bold;
    padding: 5px;
}

/* Chỉnh màu cho các mục bên trong list */
#spaCartList::item {
    border-bottom: 1px solid #EEE;
    padding: 5px;
}
/* ----- QUẢN LÝ BOOKING ----- */

/* Style cho Tab Bar */
QTabWidget::pane { 
    border: 1px solid #B2EBF2; 
    background: #FFFFFF;
}
QTabBar::tab {
    background: #E0F7FA;
    color: #006064;
    padding: 10px 20px;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    margin-right: 2px;
    font-weight: bold;
}
QTabBar::tab:selected {
    background: #FFFFFF;
    border-bottom: 2px solid #00BCD4; /* Viền xanh dưới chân tab đang chọn */
}

/* Các nút hành động */
#confirmBookingButton {
    background-color: #4CAF50; /* Xanh lá */
    color: white; font-weight: bold; border-radius: 5px; padding: 10px 20px;
}
#cancelBookingButton {
    background-color: #F44336; /* Đỏ */
    color: white; font-weight: bold; border-radius: 5px; padding: 10px 20px;
}
#completeBookingButton {
    background-color: #673AB7; /* Tím */
    color: white; font-weight: bold; border-radius: 5px; padding: 10px 20px;
}
/* Tab History */
#historyTabWidget::pane {
    border: 1px solid #CCC;
    background: white;
}
#historyTabWidget QTabBar::tab {
    background: #E0F7FA;
    color: #006064;
    padding: 8px 20px;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
}
#historyTabWidget QTabBar::tab:selected {
    background: #FFFFFF;
    border-bottom: 2px solid #009688;
    font-weight: bold;
}
/* ----- STAFF PROFILE ----- */
#profileInfoBox {
    background-color: #E0F7FA; /* Xanh nhạt (Tông màu Staff) */
    border: 1px solid #00BCD4;
    border-radius: 10px;
    padding: 30px;
    margin: 20px 50px; /* Cách lề rộng hơn */
}
#profileInfoBox QLabel {
    font-size: 16px;
    color: #006064;
    margin-bottom: 10px;
    margin-left: 60px; /* Tăng khoảng cách lề trái */
    padding-top: 5px; /* Thêm padding trên để cân đối hơn */
    padding-bottom: 5px; /* Thêm padding dưới */
}

/* Nút Edit/Save */
#editProfileButton, #saveProfileButton {
    background-color: #00BCD4; /* Xanh dương sáng */
    color: white; font-weight: bold;
    border-radius: 8px;
    padding: 10px 30px;
}
#editProfileButton:hover, #saveProfileButton:hover {
    background-color: #0097A7;
}

/* Nút Cancel */
#cancelProfileButton {
    background-color: #CFD8DC;
    color: #455A64;
    border-radius: 8px;
    padding: 10px 20px;
}</string>
