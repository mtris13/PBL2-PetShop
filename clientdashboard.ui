   <string notr="true">/* ----- CỘT TRÁI (NAV BAR) ----- */
QWidget#navBar {
    background-color: #FDD85D; /* Màu vàng chủ đạo */
    border-right: 1px solid #E0E0E0; /* Viền xám mỏng */
}
QWidget#navBar QPushButton {
    background-color: transparent; 
    border: none; 
    color: #333333; /* Chữ xám đậm (TƯỜNG MINH) */
    font-size: 16px;
    font-weight: bold;
    padding: 15px; 
    text-align: left; 
}
QWidget#navBar QPushButton:hover {
    background-color: #FBCB2E; /* Vàng đậm hơn */
}
QPushButton#petsButton {
    background-color: #FFFFFF; /* Nền trắng */
    color: #FDD85D; /* Chữ vàng (TƯỜNG MINH) */
}
#dutTitleLabel {
    font-weight: bold;
    color: #333333; /* Chữ đen (TƯỜNG MINH) */
}
#callButton, #logoutButton {
    color: #555555;
    font-size: 14px;
}

/* ----- CỘT PHẢI (CONTENT AREA) ----- */
QWidget#contentArea {
    background-color: #FFFFFF; /* Nền trắng cho nội dung chính */
}

/* Header Bar (màu xanh) */
QWidget#headerBar {
    background-color: #5D9CFD; /* Màu xanh dương */
    padding: 10px;
}
/* Chỉ QLabel BÊN TRONG headerBar mới có màu trắng */
#headerBar QLabel {
    color: #FFFFFF; /* Chữ trắng (TƯỜNG MINH) */
}
#welcomeTitleLabel, #userNameLabel {
    font-size: 18px;
    font-weight: bold;
}
#welcomeSubtitleLabel, #userRoleLabel {
    font-size: 14px;
}
#petImagesWidget QLabel {
    min-width: 40px; max-width: 40px;
    min-height: 40px; max-height: 40px;
}

/* ----- TRANG PETS (NỘI DUNG) ----- */
#petsPage {
    background-color: #FFFFFF;
}

/* Tiêu đề &quot;Dogs On Sale&quot; */
#petsTitleLabel {
    font-size: 28px;
    font-weight: bold;
    color: #333333; /* Chữ đen (TƯỜNG MINH) */
    padding: 10px;
}

/* Thanh Tìm kiếm */
#searchLineEdit {
    font-size: 15px;
    background-color: #F8F8F8; 
    border: 1px solid #E0E0E0;
    border-radius: 10px; 
    padding: 10px; 
    min-height: 25px;
    color: #333333; /* Chữ gõ vào màu đen (TƯỜNG MINH) */
}

/* Nút &quot;dog&quot; và &quot;cat&quot; */
#dogToggleButton, #catToggleButton {
    background-color: #F0F0F0; 
    color: #555555; /* Chữ xám (TƯỜNG MINH) */
    border: none;
    border-radius: 10px;
    font-weight: bold;
    font-size: 14px;
    min-height: 40px;
    min-width: 80px;
    margin-left: 5px;
}
#dogToggleButton:checked, #catToggleButton:checked {
    background-color: #FDD85D; 
    color: #333333;
}

/* ----- BẢNG THÚ CƯNG (QTableWidget) - STYLE NỀN SÁNG (QUAN TRỌNG) ----- */
QTableWidget {
    background-color: #FFFFFF; /* NỀN TRẮNG */
    color: #333333; /* CHỮ ĐEN (TƯỜNG MINH) */
    border: 1px solid #E0E0E0; 
    gridline-color: #E0E0E0; 
}
QTableWidget::item {
    padding: 10px;
    border-bottom: 1px solid #E0E0E0; 
}
QTableWidget::item:alternate {
    background-color: #F8F8F8; 
}
QTableWidget::item:!alternate {
    background-color: #FFFFFF; 
}
QTableWidget::item:selected {
    background-color: #FDD85D; 
    color: #333333; 
}

/* HEADER CỦA BẢNG (TIÊU ĐỀ NGANG) */
QHeaderView::section {
    background-color: #5D9CFD; /* Nền xanh */
    color: #FFFFFF; /* Chữ trắng (TƯỜNG MINH) */
    padding: 10px;
    font-size: 14px;
    font-weight: bold;
    border: none;
}
/* Header dọc (số 1, 2, 3...) */
QHeaderView::section:vertical {
    background-color: #F8F8F8; 
    color: #333333; /* Chữ đen (TƯỜNG MINH) */
    border-right: 1px solid #E0E0E0;
}

/* Sửa lỗi ô A(0,0) (góc trên-trái) */
QTableCornerButton::section {
    background-color: #5D9CFD; 
    border: none;
}

/* Nút &quot;ADD TO CART&quot; */
#addToCartButton {
    background-color: #5D9CFD; 
    color: #FFFFFF; /* Chữ trắng (TƯỜNG MINH) */
    border: none;
    border-radius: 10px;
    font-weight: bold;
    font-size: 16px;
    min-height: 40px; 
    margin: 10px; 
}
#addToCartButton:hover {
    background-color: #4A89E8;
}

/* Sửa lỗi QSS Cũ (nếu có) */
