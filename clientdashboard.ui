}

/* Nút &quot;REMOVE&quot; (Màu xám/bạc) */
#removeButton {
    background-color: #E0E0E0; /* Xám nhạt */
    color: #333333; /* Chữ đen */
    border: none;
    border-radius: 10px;
    font-weight: bold;
    font-size: 16px;
    min-height: 40px; 
    padding-left: 15px;
    padding-right: 15px;
    margin: 10px;
}
#removeButton:hover {
    background-color: #CCCCCC;
}

/* Nút &quot;BUY&quot; (Màu xanh) */
#buyButton {
    background-color: #5D9CFD; /* Màu xanh */
    color: #FFFFFF; 
    border: none;
    border-radius: 10px;
    font-weight: bold;
    font-size: 16px;
    min-height: 40px; 
    padding-left: 20px;
    padding-right: 20px;
    margin: 10px;
}
#buyButton:hover {
    background-color: #4A89E8;
}
/* Nút Hủy Lịch */
#cancelBookingButton {
    background-color: #FF6B6B; /* Màu đỏ nhạt */
    color: white;
    border-radius: 10px;
    font-weight: bold;
    padding: 10px 20px;
}
#cancelBookingButton:hover {
    background-color: #FF5252;
}
#bookingsTitleLabel {
    font-size: 28px;
    font-weight: bold;
    color: #333333;
    padding: 10px;
}
/* Nút Chuyển đổi Lịch sử */
#purchaseHistoryButton, #bookingHistoryButton {
    background-color: #F0F0F0; 
    color: #555555;
    border: none;
    border-radius: 10px; /* Bo tròn nhiều hơn */
    font-weight: bold;
    padding: 10px 20px;
    margin: 0 10px;
}
#purchaseHistoryButton:checked, #bookingHistoryButton:checked {
    background-color: #FDD85D; /* Màu vàng khi chọn */
    color: #333333;
}
#historyTitleLabel {
    font-size: 28px;
    font-weight: bold;
    color: #333333;
    padding: 10px;
}
/* Hộp thông tin màu vàng */
#profileInfoContainer {
    background-color: #FDD85D; /* Màu vàng */
    border-radius: 15px;
    padding: 30px;
    margin: 20px;
}
#profileInfoContainer QLabel {
    font-size: 16px;
    color: #333;
    font-weight: bold;
    margin-bottom: 10px;
margin-left: 10px;
}

/* Nút EDIT và SAVE (Màu xanh) */
#editProfileButton, #saveProfileButton {
    background-color: #5D9CFD;
    color: white;
    font-weight: bold;
    border-radius: 10px;
    min-height: 40px;
    padding: 0 30px;
}

/* Nút Cancel */
#cancelProfileButton {
    background-color: #E0E0E0;
    color: #333;
    border-radius: 10px;
    min-height: 40px;
}
QRadioButton {
    font-size: 15px;
    color: #333333; /* Chữ đen */
    padding: 5px;
    background-color: transparent;
}</string>
